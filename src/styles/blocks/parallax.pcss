.parallax__layer {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;    
}

.parallax__sky {
    object-fit: cover;
    transform: scale(2);
    width: 100%;
    height: 100%;
}

.parallax__mountains {
    top: 400px;
}

.parallax__train {
    top: 480px;
}

.parallax__grass {
    transform: scale(1.5);
    top: 1000px;
}

.parallax__img {
    position: relative;
}