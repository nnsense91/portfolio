@mixin desktopHd() {
  @media screen and (max-width: $bp-desktop-hd) {
    @content;
  }
}

@mixin desktop() {
  @media screen and (max-width: $bp-desktop) {
    @content;
  }
}

@mixin tablets() {
  @media screen and (max-width: $bp-tablets) {
    @content;
  }
}

@mixin phones() {
  @media screen and (max-width: $bp-phones) {
    @content;
  }
}

@mixin parallax-add1() {
  @media screen and (max-width: $paralax-add1) {
    @content;
  }
}

@mixin parallax-add2() {
  @media screen and (max-width: $paralax-add2) {
    @content;
  }
}

@mixin parallax-add3() {
