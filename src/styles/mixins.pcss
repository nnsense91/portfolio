    @content;
  }
}

@mixin parallax-add4() {
  @media screen and (max-width: $paralax-add4) {
    @content;
  }
}
@mixin parallax-add5() {
  @media screen and (max-width: $paralax-add5) {
    @content;
  }
}

@mixin little-phones() {
  @media screen and (max-width: $little-phones) {
